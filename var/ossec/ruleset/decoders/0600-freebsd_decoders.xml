<decoder name="userlog">
  <type>syslog</type>
  <parent>windows-date-format</parent>
  <regex>^(\d\d\d\d-\d\d-\d\d) (\d\d:\d\d:\d\d) [(\w+):(\w+)] (\.+)</regex>
  <order>date, time, user, action, extra_data</order>
</decoder>
